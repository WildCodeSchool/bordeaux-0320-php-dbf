<div class="row grey lighten-4">
    <form method="post" action="{{ path('city_edit', {'id': city.id}) }}">
        {{ form_start(form_city) }}
        <div class="input-field col s8">
            {{ form_widget(form_city.name) }}
        </div>
        <div class="col s4">
            <button class="btn modal-close light-blue btn-small " >{{ button_label|default('Sauvegarder') }}</button>
        </div>
        {{ form_end(form_city) }}
    </form>
</div>





