<form method="post" action="{{ path('service_head_delete', {'id': service_head.id}) }}" id="delService-{{service_head.id}}">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ service_head.id) }}">
    <span>
        <a class="btn mgt15 light-blue darken-1 head-deletor" data-target="delService-{{service_head.id}}">
            <i class="material-icons">delete</i>
        </a>
    </span>
</form>
